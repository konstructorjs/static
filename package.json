{
  "name": "@konstructor/static",
  "version": "0.0.0-development",
  "description": "static wrapper for konstructor",
  "main": "lib",
  "scripts": {
    "dev": "babel --out-dir lib/ --copy-files --watch src/",
    "prepublish": "npm run compile",
    "compile": "babel --out-dir lib/ --copy-files src/",
    "test": "npm run lint",
    "lint": "eslint *.js src/*",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/konstructorjs/static.git"
  },
  "keywords": [
    "konstructor",
    "koa",
    "node"
  ],
  "author": "Manthan Mallikarjun",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/konstructorjs/static/issues"
  },
  "homepage": "https://github.com/konstructorjs/static#readme",
  "dependencies": {
    "koa-static": "^4.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "travis-deploy-once": "^4.4.1",
    "semantic-release": "^15.1.5"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "publishConfig": {
    "access": "public"
  }
}
